":[0,1],"7":[1,0],"8":[1,0],"9":[0,1],"10":[0,0],"11":[0,0],"12":[0,0]},"inputSourceMap":{"version":3,"names":["QuestionController","req","res","status","json","questions","error","console","log","message","body","title","description","createAQuestion","id","uuidv4","push","question","params","foundQuestion","find","updatedQuestion","deletedQuestion","findIndex","splice","qnsId","answer","createAnAnswer","questionId","answers","foundAnswer","filter"],"sourceRoot":"D:\\Andella challenge\\stackoverflow-backend\\src\\controllers\\","sources":["questions.js"],"sourcesContent":["import { answers, questions } from '../db/db';\nimport { v4 as uuidv4 } from 'uuid';\n\nclass QuestionController {\n  static getAllQuestions(req, res) {\n    try {\n      return res.status(200).json(questions);\n    } catch (error) {\n      console.log(error.message);\n    }\n  }\n\n  static createAQuestion(req, res) {\n    try {\n      const { title, description } = req.body;\n\n      const createAQuestion = { id: uuidv4(), title, description };\n      questions.push(createAQuestion);\n\n      return res.status(201).json({\n        question: createAQuestion,\n        message: 'the question has been created',\n      });\n    } catch (error) {\n      console.log(error.message);\n    }\n  }\n\n  static getAQuestion(req, res) {\n    try {\n      const { id } = req.params;\n\n      const foundQuestion = questions.find((question) => question.id === id);\n      if (!foundQuestion) {\n        return res\n          .status(400)\n          .json({ error: 'question does not exist please check id' });\n      }\n      return res.json({ question: foundQuestion });\n    } catch (error) {\n      console.log(error.message);\n    }\n  }\n\n  static upDated(req, res) {\n    try {\n      const { id } = req.params;\n\n      const { title, description } = req.body;\n\n      const updatedQuestion = questions.find((question) => question.id === id);\n\n      if (!updatedQuestion) {\n        return res.status(201).json({ error: 'question does not exist ' });\n      }\n\n      if (title) updatedQuestion.title = title;\n\n      if (description) updatedQuestion.description = description;\n\n      return res\n        .status(201)\n        .json({ updatedQuestion, message: 'updated the question' });\n    } catch (error) {\n      console.log(error.message);\n    }\n  }\n\n  static deleteQuestion(req, res) {\n    try {\n      const { id } = req.params;\n\n      const deletedQuestion = questions.findIndex(\n        (question) => question.id === id\n      );\n      if (!deletedQuestion) {\n        return res.status(400).json({ message: 'Question not found' });\n      }\n      questions.splice(deletedQuestion, 1);\n      return res.status(204).json({ message: 'question deleted ' });\n    } catch (error) {\n      console.log(error.message);\n    }\n  }\n  //post an answer\n\n  static createAnAnswer(req, res) {\n    try {\n      const { qnsId } = req.params;\n      const { answer } = req.body;\n\n      const foundQuestion = questions.find((question) => question.id === qnsId);\n      if (!foundQuestion) {\n        return res\n          .status(400)\n          .json({ error: 'question does not exist please check id' });\n      }\n\n\n      const createAnAnswer = { id: uuidv4(), questionId: qnsId, answer: answer };\n      answers.push(createAnAnswer);\n\n      return res.status(201).json({\n        answer: createAnAnswer,\n        message: 'Answer has been created',\n      });\n    } catch (error) {\n      console.log(error.message);\n    }\n  }\n\n  static getAnAnswer(req, res) {\n    try {\n      const { id } = req.params;\n\n      const foundAnswer = answers.filter((answer) => answer.questionId === id);\n\n      if (!foundAnswer) {\n        res\n          .status(400)\n          .json({ message: 'Answer to this question does not exist' });\n      }\n      if (foundAnswer) {\n        res.status(200).json(foundAnswer);\n      }\n    } catch (error) {\n      console.log(error.message);\n    }\n  }\n}\n\nexport default QuestionController;\n"],"mappings":";;;;;;;AAAA;;AACA;;;;;;;;IAEMA,kB;;;;;;;WACJ,yBAAuBC,GAAvB,EAA4BC,GAA5B,EAAiC;MAC/B,IAAI;QACF,OAAOA,GAAG,CAACC,MAAJ,CAAW,GAAX,EAAgBC,IAAhB,CAAqBC,aAArB,CAAP;MACD,CAFD,CAEE,OAAOC,KAAP,EAAc;QACdC,OAAO,CAACC,GAAR,CAAYF,KAAK,CAACG,OAAlB;MACD;IACF;;;WAED,yBAAuBR,GAAvB,EAA4BC,GAA5B,EAAiC;MAC/B,IAAI;QACF,gBAA+BD,GAAG,CAACS,IAAnC;QAAA,IAAQC,KAAR,aAAQA,KAAR;QAAA,IAAeC,WAAf,aAAeA,WAAf;QAEA,IAAMC,eAAe,GAAG;UAAEC,EAAE,EAAE,IAAAC,QAAA,GAAN;UAAgBJ,KAAK,EAALA,KAAhB;UAAuBC,WAAW,EAAXA;QAAvB,CAAxB;;QACAP,aAAA,CAAUW,IAAV,CAAeH,eAAf;;QAEA,OAAOX,GAAG,CAACC,MAAJ,CAAW,GAAX,EAAgBC,IAAhB,CAAqB;UAC1Ba,QAAQ,EAAEJ,eADgB;UAE1BJ,OAAO,EAAE;QAFiB,CAArB,CAAP;MAID,CAVD,CAUE,OAAOH,KAAP,EAAc;QACdC,OAAO,CAACC,GAAR,CAAYF,KAAK,CAACG,OAAlB;MACD;IACF;;;WAED,sBAAoBR,GAApB,EAAyBC,GAAzB,EAA8B;MAC5B,IAAI;QACF,IAAQY,EAAR,GAAeb,GAAG,CAACiB,MAAnB,CAAQJ,EAAR;;QAEA,IAAMK,aAAa,GAAGd,aAAA,CAAUe,IAAV,CAAe,UAACH,QAAD;UAAA,OAAcA,QAAQ,CAACH,EAAT,KAAgBA,EAA9B;QAAA,CAAf,CAAtB;;QACA,IAAI,CAACK,aAAL,EAAoB;UAClB,OAAOjB,GAAG,CACPC,MADI,CACG,GADH,EAEJC,IAFI,CAEC;YAAEE,KAAK,EAAE;UAAT,CAFD,CAAP;QAGD;;QACD,OAAOJ,GAAG,CAACE,IAAJ,CAAS;UAAEa,QAAQ,EAAEE;QAAZ,CAAT,CAAP;MACD,CAVD,CAUE,OAAOb,KAAP,EAAc;QACdC,OAAO,CAACC,GAAR,CAAYF,KAAK,CAACG,OAAlB;MACD;IACF;;;WAED,iBAAeR,GAAf,EAAoBC,GAApB,EAAyB;MACvB,IAAI;QACF,IAAQY,EAAR,GAAeb,GAAG,CAACiB,MAAnB,CAAQJ,EAAR;QAEA,iBAA+Bb,GAAG,CAACS,IAAnC;QAAA,IAAQC,KAAR,cAAQA,KAAR;QAAA,IAAeC,WAAf,cAAeA,WAAf;;QAEA,IAAMS,eAAe,GAAGhB,aAAA,CAAUe,IAAV,CAAe,UAACH,QAAD;UAAA,OAAcA,QAAQ,CAACH,EAAT,KAAgBA,EAA9B;QAAA,CAAf,CAAxB;;QAEA,IAAI,CAACO,eAAL,EAAsB;UACpB,OAAOnB,GAAG,CAACC,MAAJ,CAAW,GAAX,EAAgBC,IAAhB,CAAqB;YAAEE,KAAK,EAAE;UAAT,CAArB,CAAP;QACD;;QAED,IAAIK,KAAJ,EAAWU,eAAe,CAACV,KAAhB,GAAwBA,KAAxB;QAEX,IAAIC,WAAJ,EAAiBS,eAAe,CAACT,WAAhB,GAA8BA,WAA9B;QAEjB,OAAOV,GAAG,CACPC,MADI,CACG,GADH,EAEJC,IAFI,CAEC;UAAEiB,eAAe,EAAfA,eAAF;UAAmBZ,OAAO,EAAE;QAA5B,CAFD,CAAP;MAGD,CAlBD,CAkBE,OAAOH,KAAP,EAAc;QACdC,OAAO,CAACC,GAAR,CAAYF,KAAK,CAACG,OAAlB;MACD;IACF;;;WAED,wBAAsBR,