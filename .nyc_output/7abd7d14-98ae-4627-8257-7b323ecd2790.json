rror","console","log","message","body","title","description","createAQuestion","id","uuidv4","push","question","params","foundQuestion","find","updatedQuestion","deletedQuestion","findIndex","splice","qnsId","answer","createAnAnswer","questionId","answers","foundAnswer","filter","length"],"sourceRoot":"D:\\Andella challenge\\stackoverflow-backend\\src\\controllers\\","sources":["questions.js"],"sourcesContent":["import { answers, questions } from '../db/db';\r\nimport { v4 as uuidv4 } from 'uuid';\r\n\r\nclass QuestionController {\r\n  static getAllQuestions(req, res) {\r\n    try {\r\n      return res.status(200).json(questions);\r\n    } catch (error) {\r\n      console.log(error.message);\r\n    }\r\n  }\r\n\r\n  static createAQuestion(req, res) {\r\n    try {\r\n      const { title, description } = req.body;\r\n\r\n      const createAQuestion = { id: uuidv4(), title, description };\r\n      questions.push(createAQuestion);\r\n\r\n      return res.status(201).json({\r\n        question: createAQuestion,\r\n        message: 'the question has been created',\r\n      });\r\n    } catch (error) {\r\n      console.log(error.message);\r\n    }\r\n  }\r\n\r\n  static getAQuestion(req, res) {\r\n    try {\r\n      const { id } = req.params;\r\n\r\n      const foundQuestion = questions.find((question) => question.id === id);\r\n      if (!foundQuestion) {\r\n        return res\r\n          .status(400)\r\n          .json({ error: 'question does not exist please check id' });\r\n      }\r\n      return res.json({ question: foundQuestion });\r\n    } catch (error) {\r\n      console.log(error.message);\r\n    }\r\n  }\r\n\r\n  static upDated(req, res) {\r\n    try {\r\n      const { id } = req.params;\r\n\r\n      const { title, description } = req.body;\r\n\r\n      const updatedQuestion = questions.find((question) => question.id === id);\r\n\r\n      if (!updatedQuestion) {\r\n        return res.status(400).json({ error: 'question does not exist ' });\r\n      }\r\n\r\n      if (title) updatedQuestion.title = title;\r\n\r\n      if (description) updatedQuestion.description = description;\r\n\r\n      return res\r\n        .status(201)\r\n        .json({ updatedQuestion, message: 'updated the question' });\r\n    } catch (error) {\r\n      console.log(error.message);\r\n    }\r\n  }\r\n\r\n  static deleteQuestion(req, res) {\r\n    try {\r\n      const { id } = req.params;\r\n\r\n      const deletedQuestion = questions.findIndex(\r\n        (question) => question.id === id\r\n      );\r\n      if (!deletedQuestion) {\r\n        return res.status(400).json({ message: 'Question not found' });\r\n      }\r\n      questions.splice(deletedQuestion, 1);\r\n      return res.status(204).json({ message: 'question deleted ' });\r\n    } catch (error) {\r\n      console.log(error.message);\r\n    }\r\n  }\r\n  //post an answer\r\n\r\n  static createAnAnswer(req, res) {\r\n    try {\r\n      const { qnsId } = req.params;\r\n      const { answer } = req.body;\r\n\r\n      const foundQuestion = questions.find((question) => question.id === qnsId);\r\n      if (!foundQuestion) {\r\n        return res\r\n          .status(400)\r\n          .json({ error: 'question does not exist please check id' });\r\n      }\r\n\r\n\r\n      const createAnAnswer = { id: uuidv4(), questionId: qnsId, answer: answer };\r\n      answers.push(createAnAnswer);\r\n\r\n      return res.status(201).json({\r\n        answer: createAnAnswer,\r\n        message: 'Answer has been created',\r\n      });\r\n    } catch (error) {\r\n      console.log(error.message);\r\n    }\r\n  }\r\n\r\n  static getAnAnswer(req, res) {\r\n    try {\r\n      const { id } = req.params;\r\n\r\n      const foundAnswer = answers.filter((answer) => answer.questionId === id);\r\n\r\n      if (foundAnswer.length === 0) {\r\n        return res\r\n          .status(400)\r\n          .json({ message: 'Answer to this question does not exist' });\r\n      }\r\n      return res.status(200).json(foundAnswer);\r\n\r\n    } catch (error) {\r\n      console.log(error.message);\r\n    }\r\n  }\r\n}\r\n\r\nexport default QuestionController;\r\n"],"mappings":";;;;;;;AAAA;;AACA;;;;;;;;IAEMA,kB;;;;;;;WACJ,yBAAuBC,GAAvB,EAA4BC,GAA5B,EAAiC;MAC/B,IAAI;QACF,OAAOA,GAAG,CAACC,MAAJ,CAAW,GAAX,EAAgBC,IAAhB,CAAqBC,aAArB,CAAP;MACD,CAFD,CAEE,OAAOC,KAAP,EAAc;QACdC,OAAO,CAACC,GAAR,CAAYF,KAAK,CAACG,OAAlB;MACD;IACF;;;WAED,yBAAuBR,GAAvB,EAA4BC,GAA5B,EAAiC;MAC/B,IAAI;QACF,gBAA+BD,GAAG,CAACS,IAAnC;QAAA,IAAQC,KAAR,aAAQA,KAAR;QAAA,IAAeC,WAAf,aAAeA,WAAf;QAEA,IAAMC,eAAe,GAAG;UAAEC,EAAE,EAAE,IAAAC,QAAA,GAAN;UAAgBJ,KAAK,EAALA,KAAhB;UAAuBC,WAAW,EAAXA;QAAvB,CAAxB;;QACAP,aAAA,CAAUW,IAAV,CAAeH,eAAf;;QAEA,OAAOX,GAAG,CAACC,MAAJ,CAAW,GAAX,EAAgBC,IAAhB,CAAqB;UAC1Ba,QAAQ,EAAEJ,eADgB;UAE1BJ,OAAO,EAAE;QAFiB,CAArB,CAAP;MAID,CAVD,CAUE,OAAOH,KAAP,EAAc;QACdC,OAAO,CAACC,GAAR,CAAYF,KAAK,CAACG,OAAlB;MACD;IACF;;;WAED,sBAAoBR,GAApB,EAAyBC,GAAzB,EAA8B;MAC5B,IAAI;QACF,IAAQY,EAAR,GAAeb,GAAG,CAACiB,MAAnB,CAAQJ,EAAR;;QAEA,IAAMK,aAAa,GAAGd,aAAA,CAAUe,IAAV,CAAe,UAACH,QAAD;UAAA,OAAcA,QAAQ,CAACH,EAAT,KAAgBA,EAA9B;QAAA,CAAf,CAAtB;;QACA,IAAI,CAACK,aAAL,EAAoB;UAClB,OAAOjB,GAAG,CACPC,MADI,CACG,GADH,EAEJC,IAFI,CAEC;YAAEE,KAAK,EAAE;UAAT,CAFD,CAAP;QAGD;;QACD,OAAOJ,GAAG,CAACE,IAAJ,CAAS;UAAEa,QAAQ,EAAEE;QAAZ,CAAT,CAAP;MACD,CAVD,CAUE,OAAOb,KAAP,EAAc;QACdC,OAAO,CAACC,GAAR,CAAYF,KAAK,CAACG,OAAlB;MACD;IACF;;;WAED,iBAAeR,GAAf,EAAoBC,GAApB,EAAyB;MACvB,IAAI;QACF,IAAQY,EAAR,GAAeb,GAAG,CAACiB,MAAnB,CAAQJ,EAAR;QAEA,iBAA+Bb,GAAG,CAACS,IAAnC;QAAA,IAAQC,KAAR,cAAQA,KAAR;QAAA,IAAeC,WAAf,cAAeA,WAAf;;QAEA,IAAMS,eAAe,GAAGhB,aAAA,CAAUe,IAAV,CAAe,UAACH,QAAD;UAAA,OAAcA,QAAQ,CAACH,EAAT,KAAgBA,EAA9B;QAAA,CAAf,CAAxB;;QAEA,IAAI,CAACO,eAAL,EAAsB;UACpB,OAAOnB,GAAG,CAACC,MAAJ,CAAW,GAAX,EAAgBC,IAAhB,CAAqB;YAAEE,KAAK,EAAE;UAAT,CAArB,CAAP;QACD;;QAED,IAAIK,KAAJ,EAAWU,eAAe,CAACV,KAAhB,GAAwBA,KAAxB;QAEX,IAAIC,WAAJ,EAAiBS,eAAe,CAACT,WAAhB,GAA8BA,WAA9B;QAEjB,OAAOV,GAAG,CACPC,MADI,CACG,GADH,EAEJC,IAFI,CAEC;UAAEiB,eAAe,EAAfA,eAAF;UAAmBZ,OAAO,EAAE;QAA5B,CAFD,CAAP;MAGD,CAlBD,CAkB